{
  "name": "fx250",
  "version": "0.0.0",
  "description": "Auto add lit-element custom element name to HTMLElementTagNameMap in TypeScript files",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/fx250",
  "bugs": "https://github.com/mgenware/fx250/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/fx250.git"
  },
  "bin": {
    "fx250": "./dist/main.js"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run t",
    "t": "mocha --require source-map-support/register dist_tests/**/*.test.js",
    "build": "cross-env NODE_ENV=production yarn test",
    "compile": "tsc -b tests",
    "dev": "cross-env NODE_ENV=development yarn run clean && yarn && tsc -b tests -w",
    "prepublishOnly": "yarn run test",
    "lint": "eslint --max-warnings 0 --ext .ts src/ tests/",
    "clean": "del-cli dist && del-cli dist_tests"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.21",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "mocha": "^6.2.2",
    "source-map-support": "^0.5.16",
    "temp-write": "^4.0.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "fast-glob": "^3.1.1",
    "m-fs": "^2.3.4",
    "meow": "^6.0.0"
  }
}
